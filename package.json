{
  "description": "GitBook Plugin to insert (url or file or base64) into the navigation above search input. Based on the gitbook-plugin-insert-logo plugin by matusnovak",
  "name": "gitbook-plugin-insert-logo-by-theme",
  "version": "0.0.1",
  "keywords": [
    "gitbook",
    "plugin",
    "insert-logo-by-theme",
    "insert-logo",
    "logo",
    "github"
  ],
  "main": "index.js",
  "author": {
    "name": "carlocaprini",
    "email": "7250891+carlocaprini@users.noreply.github.com"
  },
  "readme": "GitBook Plugin to insert logo (url or file or base64) into the navigation above search input.",
  "engines": {
    "gitbook": ">1.x.x"
  },
  "gitbook": {
    "properties": {
      "url": {
        "type": "string",
        "default": "http://www.example.com/my-logo.png",
        "title": "Logo URL"
      },
      "style": {
        "type": "string",
        "default": "background: none;",
        "title": "Custom CSS"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlocaprini/gitbook-plugin-insert-logo-by-theme.git"
  },
  "dependencies": {}
}
